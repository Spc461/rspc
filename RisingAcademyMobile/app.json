{
  "expo": {
    "name": "Rising Academy",
    "slug": "rising-academy-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.risingacademy.mobile"
    },
    "android": {
      "package": "com.risingacademy.mobile",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {},
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7ebfdb02-f0d8-4b28-8706-298ffbe58fb3"
      }
    }
  }
}
